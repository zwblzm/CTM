<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<package name="frame" extends="struts-default" namespace="/">
		<action name="frameAction_*" class="frameAction" method="{1}">
			<result name="left">/WEB-INF/jsps/frame/left.jsp</result>
			<result name="right">/WEB-INF/jsps/frame/right.jsp</result>
			<result name="bottom">/WEB-INF/jsps/frame/bottom.jsp</result>
			<result name="top">/WEB-INF/jsps/frame/top.jsp</result>
		
		</action>
	
	
	</package>
</struts>